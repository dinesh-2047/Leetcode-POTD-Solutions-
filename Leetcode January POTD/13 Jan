class Solution {
public:
    int minimumLength(string s) {
        int n = s.length();

        vector<int>freq(26,0);

        for(int i = 0 ; i<n; i++){
            char ch = s[i];
            int index = s[i]- 'a';

            freq[index]++;

            if(freq[index] == 3){
                freq[index]-= 2 ; 
            }
        }
        int operations = 0 ; 
        for(int i = 0; i<26; i++){
            operations += freq[i];
        }
        return operations;
    }
};